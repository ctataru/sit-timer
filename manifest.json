{
  "manifest_version": 2,
  "name": "SitTimer",
  "version": "0.1",
  "browser_action": {
    "default_title": "SitTimer",
    "default_popup": "/popup/popup.html"
  },
  "icons": {
    "16": "/images/icon.png",
    "32": "/images/icon.png",
    "48": "/images/icon.png",
    "128": "/images/icon.png"
  },
  "background" : {
    "scripts" : ["background.js"],
    "persistent": false
  },
  "permissions": [
    "notifications",
    "storage"
  ]
}
