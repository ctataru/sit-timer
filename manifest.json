{
  "manifest_version": 2,
  "name": "SitTimer",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["index.js"]
    }
  ],
  "browser_action": {
    "default_title": "testing 123",
    "default_popup": "/popup/popup.html"
  },
  "icons": {
    "16": "/images/icon.png",
    "32": "/images/icon.png",
    "48": "/images/icon.png",
    "128": "/images/icon.png"
  }
}
